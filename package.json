{
  "name": "healrag-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Intelligent Code Generator Frontend - React TypeScript application for AI-powered code generation",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite --host localhost --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "test:backend": "curl -f https://healrag-security.azurewebsites.net/health || echo 'Backend not available'",
    "clean": "rm -rf dist node_modules/.vite",
    "start": "npm run dev"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.26.1",
    "@azure/msal-react": "^2.2.0",
    "@fluentui/react": "^8.112.5",
    "@fluentui/react-components": "^9.56.2",
    "@fluentui/react-icons": "^2.0.265",
    "@react-spring/web": "^9.7.5",
    "dompurify": "^3.2.4",
    "i18next": "^24.2.0",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.1",
    "idb": "^8.0.0",
    "ndjson-readablestream": "^1.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.4.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.28.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "scheduler": "^0.20.2"
  },
  "devDependencies": {
    "@types/dom-speech-recognition": "^0.0.4",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.3.3",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.18"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": [
    "intelligent",
    "code",
    "generator",
    "ai",
    "chat",
    "frontend",
    "react",
    "typescript"
  ],
  "author": "HEALRAG Team",
  "license": "MIT"
}
